<div id="<%= dom_id category %>" class="bg-white shadow-sm rounded-3 py-2 mx-2 row" style="min-height: 100px">
  <div class="col-4 d-flex align-items-center">
    <%= image_tag category.icon.variant(resize_to_fit: [100,100])%>
  </div>
  <div class="col-6 pt-1 d-flex flex-column justify-content-between text-muted">
    <span >
    <%= category.name %>
    </span>
    <%= category.created_at.strftime("%d %b %Y") %>
  </div>
  <div class="col-2 px-0 pt-1">
    <%# render total payements %>
    $100
  </div>
</div>
